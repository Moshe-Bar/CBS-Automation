/* קובץ זה משויך כעת לקובץ HTML בעל שם זהה והוא שואב ממנו תוכן. עד לביטול השיוך בין הקבצים, לא תוכל להעביר את הקובץ, למחוק אותו, לשנות את שמו או לבצע בו שינויים אחרים. */

function DisplayTemplate_0800588569ee478196bbc4407ea3fcb5(ctx) {
  var ms_outHtml=[];
  var cachePreviousTemplateData = ctx['DisplayTemplateData'];
  ctx['DisplayTemplateData'] = new Object();
  DisplayTemplate_0800588569ee478196bbc4407ea3fcb5.DisplayTemplateData = ctx['DisplayTemplateData'];

  ctx['DisplayTemplateData']['TemplateUrl']='~sitecollection\u002f_catalogs\u002fmasterpage\u002fDisplay Templates\u002fContent Web Parts\u002fCbsControl.js';
  ctx['DisplayTemplateData']['TemplateType']='Control';
  ctx['DisplayTemplateData']['TargetControlType']=['Content Web Parts'];
  this.DisplayTemplateData = ctx['DisplayTemplateData'];

ms_outHtml.push('',''
,''
); 
    if (!$isNull(ctx.ClientControl) &&
        !$isNull(ctx.ClientControl.shouldRenderControl) &&
        !ctx.ClientControl.shouldRenderControl())
    {
        return "";
    }
    ctx.ListDataJSONGroupsKey = "ResultTables";
    var $noResults = Srch.ContentBySearch.getControlTemplateEncodedNoResultsMessage(ctx.ClientControl);

    var noResultsClassName = "ms-srch-result-noResults";

    var ListRenderRenderWrapper = function(itemRenderResult, inCtx, tpl)
    {
        var iStr = [];
        iStr.push('<li>');
        iStr.push(itemRenderResult);
        iStr.push('</li>');
        return iStr.join('');
    }
    ctx['ItemRenderWrapper'] = ListRenderRenderWrapper;

    var BottomLinkKey = ctx.DataProvider.get_properties()["BottomLinkKey"] || "no  BottomLinkKey ";
    var bottomLink = '';
    if (UIResourceLinks[BottomLinkKey] === undefined){
        console.error("UIResourceLinks[BottomLinkKey] is undefined, key = " + BottomLinkKey);
    }
    else{
        bottomLink = UIResourceLinks[BottomLinkKey]
    }

    AddPostRenderCallback(ctx, function()
    {              
         $(".cbs-Item-green").parents(".ms-webpart-chrome").addClass("pressReleasesBoxGreen");
        $.each($(".cbs-Item-green"), function( index, greenBox) {
	    if( $(greenBox).parents(".cbs-List").nextAll('.hrDotted2:first').length != 0)
		    return;
	    var greenUL = $(greenBox).parents(".cbs-List");
	    $('<hr class="hrDotted2"/>').insertAfter(greenUL);
    });



});

ms_outHtml.push(''
,''
,'<div class="cbs-control">'
,'    <ul class="cbs-List">'
,'         ', ctx.RenderGroups(ctx) ,''
,'    </ul>'
,'    ', bottomLink ,''
,'</div>    '
,''
,'                '
);



if (ctx.ClientControl.get_shouldShowNoResultMessage())
{
ms_outHtml.push(''
,'        <div class="', noResultsClassName ,'">', $noResults ,'</div>'
);
}
ms_outHtml.push(''
,''
,'    '
);

  ctx['DisplayTemplateData'] = cachePreviousTemplateData;
  return ms_outHtml.join('');
}
